      "mocha"
    ]
  },
  "author": "Yuanlei Hong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hongyuanlei/webpack-path-override-plugin/issues"
  },
  "homepage": "https://github.com/hongyuanlei/webpack-path-override-plugin#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^3.12.2",
    "mocha": "^3.2.0"
  }
}
