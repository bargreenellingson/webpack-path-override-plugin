{
  "name": "webpack-path-override-plugin",
  "version": "1.0.5",
  "description": "provide a webpack plugin to override import or require path",
  "main": "index.js",
  "scripts": {
    "build": "npm run eslint && npm run test && babel src/webpack-path-override-plugin.js -o index.js",
    "prepublish": "npm run build",
    "eslint": "eslint src",
    "test": "mocha --compilers js:babel-core/register test/**/*-spec.js"
